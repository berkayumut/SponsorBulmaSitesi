@model IEnumerable<SponsorBulmaSitesi.TblFenomenler>
<div class="container">
    <div class="text-center">
        <h2 class="section-heading text-uppercase">ÇEKİLİŞ YAPILAN İNSTAGRAM HESAPLARI</h2>
        <h3 class="section-subheading text-muted">Yaklaşık 60 hesaptan sadece bazıları</h3>
    </div>
    <div class="row">
        @foreach (var item in Model)
        {

            <div class="col-lg-4">
                <div class="team-member">
                    <a href="@item.InstaUrl"><img src="~/Images/FenomenResimleri/@item.Foto" class="mx-auto rounded-circle"/></a>
                    <h4>@item.FenomenAdi</h4>
                    <p class="text-muted"> <b>@item.TakipciSayisi</b> Takipçi</p>

                </div>
            </div>

        }
    </div>
</div>
