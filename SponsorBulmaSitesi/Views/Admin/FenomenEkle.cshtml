

@{
    ViewBag.Title = "PaketEkle";
}
<body>
    <div class="container" style="margin-top:10px">
        <h3>Fenomen Ekle</h3>
        <form action="/Admin/FenomenEkle" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label class="col-md-2 control-label">Fenomen Adı: </label>
                <div class="col-md-10">
                    <input type="text" name="adi" value="" placeholder="Adınızı Giriniz" class="form-control" />
                </div>

            </div>
            <div class="form-group">
                <label class="col-md-2 control-label">Takipçi Sayısı: </label>
                <div class="col-md-10">
                    <input type="text" name="takipciSayisi" value="" placeholder="Takipçi Sayısını Giriniz" class="form-control" />
                </div>
 
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label">Takipçi Yollama Potansiye: </label>
                <div class="col-md-10">
                    <input type="text" name="takipciKapasite" value="" placeholder="Tahmini Takipçi Kazandırma Sayısı" class="form-control" />
                </div>

            </div>
            <div class="form-group">
                <label class="col-md-2 control-label">URL: </label>
                <div class="col-md-10">
                    <input type="text" name="instaurl" value="" placeholder="URL" class="form-control" />
                </div>

            </div>
            <div class="form-group">
                <label class="col-md-2 control-label">Resim: </label>
                <div class="col-md-10">
                    <input type="file" name="Foto" accept="image/*" />

                </div>
            </div>

            <div class="form-group">
                <input type="submit" class="btn-success" name="name" value="Kaydet" />

            </div>
        </form>

    </div>
</body>





